<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.fisk.dataaccess.mapper.TableAccessMapper">
    <select id="filter" resultType="com.fisk.dataaccess.dto.TableAccessNonDTO">
        select id,
        table_name,
        table_des,
        update_time
        from tb_app_registration
        where del_flag=1
        <if test="query.where !=null and query.where.length()>0 ">
            ${query.where}
        </if>
    </select>
</mapper>